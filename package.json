{
  "name": "simple-socket-chat",
  "version": "1.0.0",
  "description": "my first socket.io app",
  "main": "index.js",
  "scripts": {
    "start:debugger": "node --inspect=0.0.0.0:9229 index.js",
    "server": "nodemon index.js",
    "client": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --open",
    "start": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "css-loader": "^0.28.11",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "html-webpack-plugin": "^2.30.1",
    "concurrently": "^3.6.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.12",
    "nodemon": "^1.18.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "2.1.1"
  }
